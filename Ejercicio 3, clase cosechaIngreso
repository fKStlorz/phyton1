class cosechaingreso:
    __id=0
    __dia=0
    __peso=0.0
    def __init__(self,id,dia,peso):
        self.__id=id
        self.__dia=dia
        self.__peso=peso

    def getId(self):
        return int(self.__id)

    def getDia(self):
        return self.__dia

    def getPeso(self):
        return int(self.__peso)
